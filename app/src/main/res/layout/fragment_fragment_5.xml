<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.bwf.tuanche.home_page.fragment.Fragment_5">


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/simple_black"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.5"
            android:orientation="horizontal">
            <RelativeLayout
                android:id="@+id/rl_hunyinzuojia"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_hunyin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="测试"
                    android:textSize="18sp"
                    android:textColor="@color/yellow"
                    android:textStyle="bold"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:id="@+id/tv_zuihao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/tv_hunyin"
                    android:layout_marginTop="10dp"/>
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/img_hunyin"
                    android:layout_width="80dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="10dp"
                    android:layout_below="@id/tv_zuihao"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/loading_left"/>
            </RelativeLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/simple_black"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1">
                <RelativeLayout
                    android:id="@+id/rl_mamigouche"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">
                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/img_gouche"
                        android:layout_width="60dp"
                        android:layout_height="50dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@mipmap/icon_quoting_price_payback"/>
                    <TextView
                        android:id="@+id/tv_gouche"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="妈咪购车"
                        android:textSize="18sp"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="20dp"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/tv_wenxin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="温馨驾道"
                        android:textSize="16sp"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_below="@id/tv_gouche" />


                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/simple_black"/>
                <RelativeLayout
                    android:id="@+id/rl_zijialvxing"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">
                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/img_lvxing"
                        android:layout_width="60dp"
                        android:layout_height="50dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@mipmap/icon_quoting_price_payback"/>
                    <TextView
                        android:id="@+id/tv_lvxing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="自驾旅行"
                        android:textSize="18sp"
                        android:textColor="@color/blue"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="20dp"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/tv_zou"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="说走就走"
                        android:textSize="16sp"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_below="@id/tv_lvxing" />

                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/simple_black"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:id="@+id/rl_shangwuzhuangxiang"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_shangwu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="16sp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:id="@+id/tv_yilu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="14sp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/tv_shangwu"
                    android:layout_marginTop="5dp"/>
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/img_shangwu"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="5dp"
                    android:layout_below="@id/tv_yilu"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/icon_zhonggaoji_sel"/>

            </RelativeLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/simple_black"/>
            <RelativeLayout
                android:id="@+id/rl_bailingEzu"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_bailing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="16sp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:id="@+id/tv_zizai"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="14sp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/tv_bailing"
                    android:layout_marginTop="5dp"/>
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/img_bailing"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="5dp"
                    android:layout_below="@id/tv_zizai"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/icon_zhonggaoji_sel"/>


            </RelativeLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/simple_black"/>
            <RelativeLayout
                android:id="@+id/rl_daibuyoudao"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_daibu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="16sp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:id="@+id/tv_sheng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="婚姻座驾"
                    android:textSize="14sp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/tv_daibu"
                    android:layout_marginTop="5dp"/>
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/img_daibu"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="5dp"
                    android:layout_below="@id/tv_sheng"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/icon_zhonggaoji_sel"/>


            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/simple_black"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/simple_black"/>
</LinearLayout>
